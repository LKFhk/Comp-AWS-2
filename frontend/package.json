    "cypress-axe": "^1.5.0",
    "eslint-plugin-storybook": "^0.6.15",
    "http-proxy-middleware": "^3.0.5",
    "jest-axe": "^8.0.0",
    "lighthouse": "^12.8.2",
    "prop-types": "^15.8.1",
    "react-refresh": "^0.18.0",
    "react-scripts": "^5.0.1",
