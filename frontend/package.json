    "typescript": "^4.9.5",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "scripts": {
    "start": "set NODE_OPTIONS=--no-deprecation && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false --collectCoverageFrom='src/**/*.{js,jsx,ts,tsx}' --collectCoverageFrom='!src/**/*.stories.{js,jsx,ts,tsx}' --collectCoverageFrom='!src/index.tsx' --collectCoverageFrom='!src/reportWebVitals.ts'",
    "test:a11y": "react-scripts test --testPathPattern=accessibility --watchAll=false",
    "test:performance": "react-scripts test --testPathPattern=performance --watchAll=false",
    "test:unit": "react-scripts test --testPathPattern=\"\\.(test|spec)\\.(js|jsx|ts|tsx)$\" --testPathIgnorePatterns=\"cypress|playwright|e2e\" --watchAll=false",
    "test:visual": "percy exec -- cypress run",
    "test:lighthouse": "lhci autorun",
    "eject": "react-scripts eject",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:headless": "cypress run --headless",
    "playwright:test": "playwright test",
    "playwright:install": "playwright install",
    "playwright:test:chromium": "playwright test --project=chromium",
    "playwright:test:firefox": "playwright test --project=firefox",
    "playwright:test:webkit": "playwright test --project=webkit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --exit-zero-on-changes",
    "test:all": "npm run test:unit && npm run test:a11y && npm run test:performance && npm run cypress:run:headless && npm run playwright:test",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "build:analyze": "npm run build && echo 'Build completed. Check build folder for bundle analysis.'",
    "cypress:version": "cypress version",
    "test:cypress:15": "cypress run --config-file cypress.config.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "nth-check": "^2.1.1",
    "postcss": "^8.4.31",
    "webpack-dev-server": "^4.15.1",
    "ws": "^8.18.0",
    "tmp": "^0.2.4",
    "esbuild": "^0.25.0"
  }
}
